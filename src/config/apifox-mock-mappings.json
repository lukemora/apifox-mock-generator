{
  "string": {
    "{{$string.uuid}}": "@guid",
    "{{$string.sample}}": "@cword(3, 8)",
    "{{$string.alphanumeric}}": "@word(3, 8)",
    "{{$string.alpha}}": "@cword(3, 8)",
    "{{$string.numeric}}": "@integer(100000, 999999)",
    "{{$string.hex}}": "@hex",
    "{{$string.base64}}": "@base64"
  },
  "person": {
    "{{$person.fullName}}": "@cname",
    "{{$person.fullName(locale='zh_CN')}}": "@cname",
    "{{$person.fullName(locale='en_US')}}": "@name",
    "{{$person.firstName}}": "@cfirst",
    "{{$person.lastName}}": "@clast",
    "{{$person.name}}": "@cname",
    "{{$person.gender}}": "@pick([\"男\", \"女\"])",
    "{{$person.age}}": "@integer(18, 80)",
    "{{$person.birthday}}": "@date(\"yyyy-MM-dd\")",
    "{{$person.avatar}}": "@image(\"200x200\", \"@color\", \"@cname\")"
  },
  "internet": {
    "{{$internet.email}}": "@email",
    "{{$internet.url}}": "@url",
    "{{$internet.ip}}": "@ip",
    "{{$internet.ipv6}}": "@ipv6",
    "{{$internet.domain}}": "@domain",
    "{{$internet.username}}": "@word(3, 10)",
    "{{$internet.password}}": "@word(8, 16)",
    "{{$internet.userAgent}}": "@userAgent",
    "{{$internet.color}}": "@color",
    "{{$internet.hexColor}}": "@hex",
    "{{$internet.rgbColor}}": "@rgb",
    "{{$internet.hslColor}}": "@hsl"
  },
  "phone": {
    "{{$phone.number}}": "/^1[3-9]\\d{9}$/",
    "{{$phone.phoneNumber}}": "/^1[3-9]\\d{9}$/",
    "{{$phone.landline}}": "/^0\\d{2,3}-?\\d{7,8}$/"
  },
  "address": {
    "{{$address.city}}": "@city",
    "{{$address.province}}": "@province",
    "{{$address.county}}": "@county",
    "{{$address.country}}": "@country",
    "{{$address.street}}": "@street",
    "{{$address.zipCode}}": "@zip",
    "{{$address.latitude}}": "@float(-90, 90, 6, 6)",
    "{{$address.longitude}}": "@float(-180, 180, 6, 6)",
    "{{$address.fullAddress}}": "@county(true) @city(true) @street(true)"
  },
  "date": {
    "{{$date.now}}": "@now",
    "{{$date.recent}}": "@datetime",
    "{{$date.anytime}}": "@datetime",
    "{{$date.past}}": "@datetime",
    "{{$date.future}}": "@datetime",
    "{{$date.between}}": "@datetime",
    "{{$date.anytime|format('HH:mm:ss')}}": "@time(\"HH:mm:ss\")",
    "{{$date.anytime|format('yyyy-MM-dd')}}": "@date(\"yyyy-MM-dd\")",
    "{{$date.anytime|format('yyyy-MM-dd HH:mm:ss')}}": "@datetime(\"yyyy-MM-dd HH:mm:ss\")",
    "{{$date.anytime|format('MM/dd/yyyy')}}": "@date(\"MM/dd/yyyy\")",
    "{{$date.anytime|format('dd/MM/yyyy')}}": "@date(\"dd/MM/yyyy\")",
    "{{$date.anytime|format('yyyy年MM月dd日')}}": "@date(\"yyyy年MM月dd日\")",
    "{{$date.anytime|format('HH:mm')}}": "@time(\"HH:mm\")",
    "{{$date.anytime|format('yyyy')}}": "@date(\"yyyy\")",
    "{{$date.anytime|format('MM')}}": "@date(\"MM\")",
    "{{$date.anytime|format('dd')}}": "@date(\"dd\")",
    "{{$date.anytime|format('HH')}}": "@time(\"HH\")",
    "{{$date.anytime|format('mm')}}": "@time(\"mm\")",
    "{{$date.anytime|format('ss')}}": "@time(\"ss\")"
  },
  "number": {
    "{{$number.int}}": "@integer(0, 100)",
    "{{$number.float}}": "@float(0, 100, 2, 2)",
    "{{$number.double}}": "@float(0, 100, 2, 2)",
    "{{$number.decimal}}": "@float(0, 100, 2, 2)",
    "{{$number.positive}}": "@integer(1, 1000)",
    "{{$number.negative}}": "@integer(-1000, -1)",
    "{{$number.digit}}": "@integer(0, 9)",
    "{{$number.boolean}}": "@boolean",
    "{{$number.bit}}": "@pick([0, 1])",
    "{{$number.octal}}": "@integer(0, 7)",
    "{{$number.hex}}": "@hex"
  },
  "boolean": {
    "{{$boolean}}": "@boolean",
    "{{$boolean.true}}": "true",
    "{{$boolean.false}}": "false"
  },
  "image": {
    "{{$image.image}}": "@image",
    "{{$image.avatar}}": "@image(\"200x200\", \"@color\", \"@cname\")",
    "{{$image.dataUri}}": "@dataImage",
    "{{$image.placeholder}}": "@image(\"300x200\", \"@color\", \"@cword\")"
  },
  "lorem": {
    "{{$lorem.word}}": "@word",
    "{{$lorem.words}}": "@cword(3, 8)",
    "{{$lorem.sentence}}": "@csentence(5, 15)",
    "{{$lorem.sentences}}": "@csentence(3, 8)",
    "{{$lorem.paragraph}}": "@cparagraph(1, 3)",
    "{{$lorem.paragraphs}}": "@cparagraph(2, 5)",
    "{{$lorem.text}}": "@cparagraph",
    "{{$lorem.lines}}": "@cparagraph(1, 3)"
  },
  "color": {
    "{{$color.color}}": "@color",
    "{{$color.hex}}": "@hex",
    "{{$color.rgb}}": "@rgb",
    "{{$color.hsl}}": "@hsl",
    "{{$color.hsv}}": "@hsv",
    "{{$color.rgba}}": "@rgba",
    "{{$color.hsla}}": "@hsla"
  },
  "file": {
    "{{$file.fileName}}": "@word(3, 10).@word(2, 4)",
    "{{$file.fileExt}}": "@pick([\"jpg\", \"png\", \"gif\", \"pdf\", \"doc\", \"txt\"])",
    "{{$file.mimeType}}": "@pick([\"image/jpeg\", \"image/png\", \"application/pdf\", \"text/plain\"])"
  },
  "random": {
    "{{$random.uuid}}": "@guid",
    "{{$random.id}}": "@integer(1, 1000000)",
    "{{$random.hash}}": "@hex",
    "{{$random.token}}": "@word(32)",
    "{{$random.creditCard}}": "@creditCard",
    "{{$random.creditCardNumber}}": "@creditCardNumber",
    "{{$random.creditCardType}}": "@pick([\"Visa\", \"MasterCard\", \"American Express\"])",
    "{{$random.currency}}": "@pick([\"CNY\", \"USD\", \"EUR\", \"JPY\"])",
    "{{$random.currencyCode}}": "@pick([\"CNY\", \"USD\", \"EUR\", \"JPY\"])",
    "{{$random.language}}": "@pick([\"zh-CN\", \"en-US\", \"ja-JP\", \"ko-KR\"])",
    "{{$random.locale}}": "@pick([\"zh_CN\", \"en_US\", \"ja_JP\", \"ko_KR\"])"
  }
}
