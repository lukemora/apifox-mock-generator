{
  "name": "apifox-mock-generator",
  "version": "2.0.2",
  "description": "从Apifox拉取API接口并生成本地mock和TypeScript类型文件，并提供mock服务器",
  "main": "dist/src/index.js",
  "type": "module",
  "bin": {
    "apifox-mock": "dist/src/scripts/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "generate": "node dist/src/scripts/generate-mock.js",
    "serve": "node dist/src/scripts/serve-mock.js",
    "dev": "npm run generate && npm run serve",
    "dev:generate": "tsx src/scripts/generate-mock.ts",
    "dev:serve": "tsx src/scripts/serve-mock.ts",
    "dev:full": "npm run dev:generate && npm run dev:serve",
    "test:e2e": "vitest run tests/e2e",
    "test:e2e:config": "vitest run tests/e2e/apifox-config.e2e.test.ts tests/e2e/generated-files.e2e.test.ts",
    "test:e2e:mock": "vitest run tests/e2e/mock-config.e2e.test.ts tests/e2e/mock-server.e2e.test.ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "apifox",
    "mock",
    "api",
    "typescript",
    "mock-server",
    "openapi",
    "code-generator"
  ],
  "author": "liujun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukemora/apifox-mock-generator.git"
  },
  "homepage": "https://github.com/lukemora/apifox-mock-generator#readme",
  "bugs": {
    "url": "https://github.com/lukemora/apifox-mock-generator/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "chokidar": "^4.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-vue": "^10.5.1",
    "glob": "^11.0.3",
    "puppeteer": "^24.34.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "apifox-cli": "^1.2.0",
    "axios": "^1.6.5",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "prettier": "^3.6.2"
  }
}
